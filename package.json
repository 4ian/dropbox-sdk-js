{
  "name": "dropbox-sdk",
  "version": "0.0.1",
  "description": "Dropbox API v2 JavaScript client.",
  "main": "index.js",
  "browser": "./dist/dropbox-sdk.js",
  "scripts": {
    "build": "npm run browserify && npm run uglify && npm run compressed-size",
    "watch": "watchify src/browser.js -d -v -s DropboxApi -o dist/dropbox-sdk.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify src/browser.js --standalone DropboxApi -o dist/dropbox-sdk.js",
    "uglify": "uglifyjs dist/dropbox-sdk.js -mc 'warnings=false' -o dist/dropbox-sdk.min.js",
    "compressed-size": "echo 'dropbox-sdk.min.js gzipped will weigh' $(cat dist/dropbox-sdk.min.js | gzip -9 | wc -c | pretty-bytes)"
  },
  "keywords": [
    "dropbox"
  ],
  "author": "Riley Tomasek",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.0.0",
    "pretty-bytes": "^2.0.1",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "es6-promise": "^3.1.2",
    "request": "^2.69.0"
  }
}
