{
  "name": "dropbox-sdk",
  "version": "0.0.1",
  "description": "Dropbox JavaScript SDK.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-cjs && npm run build-umd && npm run compressed-size",
    "build-cjs": "webpack",
    "build-umd": "webpack -p --config webpack-umd.config.js",
    "compressed-size": "echo 'dropbox-sdk.min.js gzipped will weigh' $(cat dist/dropbox-sdk.min.js | gzip -9 | wc -c | pretty-bytes)",
    "lint": "eslint src --ignore-pattern src/routes.js",
    "start": "node examples/server.js",
    "test": "npm run lint && mocha 'test/*.js'"
  },
  "keywords": [
    "dropbox"
  ],
  "author": "Riley Tomasek",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "express": "^4.13.4",
    "express-urlrewrite": "^1.2.0",
    "mocha": "^2.4.5",
    "pretty-bytes": "^2.0.1",
    "prompt": "^1.0.0",
    "sinon": "^1.17.3",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "dependencies": {
    "es6-promise": "^3.1.2",
    "superagent": "^1.8.3"
  }
}
